<!-- landmarks/index -->

<h1>Landmarks:</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Year Completed</th>
  </tr>
  <% @landmarks.each do |lm| %>
    <tr>
      <td><a href="/landmarks/<%=lm.id%>"><%=lm.name%></a></td>
      <td><%=lm.year_completed%></td>
    </tr>
  <% end %>
<table>
<br>

<h2>Create a <a href="/landmarks/new">New Landmark</a></h2>
